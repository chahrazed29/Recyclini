<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EntrepriseActivity"
    android:background="@drawable/bg3">

    <EditText
        android:id="@+id/editText8"
        android:layout_width="230dp"
        android:layout_height="40dp"

        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"


        android:layout_marginBottom="50dp"
        android:alpha="0.4"

        android:background="@drawable/input_field"
        android:cursorVisible="true"
        android:ems="10"
        android:hint="Nom Entreprise"
        android:inputType="textPersonName"
        android:textColor="#0A0A0A"
        android:textColorHighlight="#3E000000"
        android:textColorLink="@color/white"
        android:textCursorDrawable="@drawable/input_field"
        android:textIsSelectable="true"
        app:layout_constraintBottom_toTopOf="@+id/editText10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="230dp"
        android:layout_height="40dp"

        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"

        android:layout_marginRight="16dp"
        android:layout_marginBottom="50dp"
        android:alpha="0.4"
        android:background="@drawable/input_field"
        android:cursorVisible="true"
        android:ems="10"
        android:hint="N registre de commerce"


        android:inputType="text"
        android:textColor="#0A0A0A"
        android:textColorHighlight="#3E000000"
        android:textColorLink="@color/white"
        android:textCursorDrawable="@drawable/input_field"
        android:textIsSelectable="true"
        app:layout_constraintBottom_toTopOf="@+id/editText11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="230dp"
        android:layout_height="40dp"

        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"

        android:layout_marginBottom="50dp"
        android:alpha="0.4"
        android:background="@drawable/input_field"
        android:cursorVisible="true"


        android:ems="10"


        android:hint="Email"
        android:inputType="textEmailAddress"
        android:textColor="#0A0A0A"
        android:textColorHighlight="#3E000000"
        android:textColorLink="@color/white"
        android:textCursorDrawable="@drawable/input_field"
        android:textIsSelectable="true"
        app:layout_constraintBottom_toTopOf="@+id/editText13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="230dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="50dp"
        android:alpha="0.4"
        android:background="@drawable/input_field"
        android:cursorVisible="true"
        android:ems="10"
        android:hint="Mot de passe"
        android:inputType="textPassword"
        android:textColor="#0A0A0A"
        android:textColorHighlight="#3E000000"
        android:textColorLink="@color/white"
        android:textCursorDrawable="@drawable/input_field"
        android:textIsSelectable="false"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button3"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/button2"
        android:fontFamily="@font/regular"

        android:text="S'inscrire"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="19dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />
   <!-- android:onClick="buloginckic" -->
    <ImageView
        android:id="@+id/backbtn2"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="6dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"/>
       <!-- app:srcCompat="@drawable/retoure" -->

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="340dp"
        android:layout_height="164dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="13dp"
        android:layout_marginRight="13dp"
        app:layout_constraintBottom_toTopOf="@+id/editText8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logoandname" />

</androidx.constraintlayout.widget.ConstraintLayout>